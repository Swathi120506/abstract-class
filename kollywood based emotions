      "cell_type": "code",
      "source": [
        "%%writefile app.py\n",
        "import streamlit as st\n",
        "import pandas as pd\n",
        "import nltk\n",
        "import string\n",
        "from nltk.corpus import stopwords\n",
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "from sklearn.naive_bayes import MultinomialNB\n",
        "\n",
        "# ---------- DATASET (Data Science) ----------\n",
        "data = {\n",
        "    \"text\": [\n",
        "        \"thalapathy mass dialogues\",\n",
        "        \"vada chennai intense scenes\",\n",
        "        \"arr melody songs\",\n",
        "        \"anirudh bgm gives goosebumps\",\n",
        "        \"romantic love songs\",\n",
        "        \"sad emotional dialogues\"\n",
        "    ],\n",
        "    \"emotion\": [\n",
        "        \"Energetic\",\n",
        "        \"Emotional\",\n",
        "        \"Calm\",\n",
        "        \"Energetic\",\n",
        "        \"Romantic\",\n",
        "        \"Sad\"\n",
        "    ],\n",
        "    \"personality\": [\n",
        "        \"Mass Lover\",\n",
        "        \"Intense Thinker\",\n",
        "        \"Calm Mind\",\n",
        "        \"Energetic Soul\",\n",
        "        \"Romantic Heart\",\n",
        "        \"Emotional Soul\"\n",
        "    ]\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# ---------- TEXT CLEANING ----------\n",
        "stop_words = stopwords.words('english')\n",
        "\n",
        "def clean_text(text):\n",
        "    text = text.lower()\n",
        "    text = \"\".join(c for c in text if c not in string.punctuation)\n",
        "    text = \" \".join(w for w in text.split() if w not in stop_words)\n",
        "    return text\n",
        "\n",
        "df[\"clean_text\"] = df[\"text\"].apply(clean_text)\n",
        "\n",
        "# ---------- AI MODEL ----------\n",
        "vectorizer = TfidfVectorizer()\n",
        "X = vectorizer.fit_transform(df[\"clean_text\"])\n",
        "y_emotion = df[\"emotion\"]\n",
        "y_personality = df[\"personality\"]\n",
        "\n",
        "emotion_model = MultinomialNB()\n",
        "emotion_model.fit(X, y_emotion)\n",
        "\n",
        "personality_model = MultinomialNB()\n",
        "personality_model.fit(X, y_personality)\n",
        "\n",
        "# ---------- WEBSITE UI ----------\n",
        "st.set_page_config(page_title=\"Kollywood AI Analyzer\", layout=\"centered\")\n",
        "\n",
        "st.title(\"ðŸŽ¬ Kollywood AI Personality Analyzer\")\n",
        "st.write(\"Enter your favourite Kollywood movies, songs or dialogues\")\n",
        "\n",
        "user_input = st.text_area(\"âœï¸ Your favourite content\")\n",
        "\n",
        "if st.button(\"Analyze\"):\n",
        "    if user_input.strip():\n",
        "        cleaned = clean_text(user_input)\n",
        "        vector = vectorizer.transform([cleaned])\n",
        "\n",
        "        emotion = emotion_model.predict(vector)[0]\n",
        "        personality = personality_model.predict(vector)[0]\n",
        "\n",
        "        score = round(max(emotion_model.predict_proba(vector)[0]) * 100, 2)\n",
        "\n",
        "        st.success(\"âœ… Analysis Complete\")\n",
        "        st.write(\"### ðŸ§  Personality:\", personality)\n",
        "        st.write(\"### ðŸ˜Š Emotion:\", emotion)\n",
        "        st.write(\"### ðŸ“Š Confidence Score:\", f\"{score}%\")\n",
        "    else:\n",
        "        st.warning(\"Please enter some text\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "uza_zeAZ35mt",
